                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
                        </svg>
                        Logs
                    </button>
                </div>
            </div>

            <!-- Anna's Archive Collector -->
            <div class="collector-card stopped">
                <div class="collector-header">
                    <div class="collector-info">
                        <h3 class="collector-name">
                            <svg class="collector-icon" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 14v3m4-3v3m4-3v3M3 21h18M3 10h18M3 7l9-4 9 4M4 10v11M20 10v11"/>
                            </svg>
                            Anna's Archive Collector
                        </h3>
                        <p class="collector-description">Searches and downloads financial textbooks and research materials from Anna's Archive</p>
                        <div class="collector-tags">
                            <span class="collector-tag">Books</span>
                            <span class="collector-tag">Archive</span>
                            <span class="collector-tag">Academic</span>
                        </div>
                    </div>
                    <div class="collector-status">
                        <span class="status-indicator status-stopped"></span>
                        <span>Stopped</span>
                    </div>
                </div>

                <div class="collector-metrics">
                    <div class="metric-item">
                        <span class="metric-value">89</span>
                        <span class="metric-label">Documents</span>
                    </div>
                    <div class="metric-item">
                        <span class="metric-value">234.7 MB</span>
                        <span class="metric-label">Size</span>
                    </div>
                    <div class="metric-item">
                        <span class="metric-value">Idle</span>
                        <span class="metric-label">Status</span>
                    </div>
                </div>

                <div class="collector-controls">
                    <button class="collector-btn start">
                        <svg width="12" height="12" fill="currentColor" viewBox="0 0 24 24">
                            <path d="M8 5v14l11-7z"/>
                        </svg>
                        Start
                    </button>
                    <button class="collector-btn configure">
                        <svg width="12" height="12" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.5 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"/>
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"/>
                        </svg>
                        Configure
                    </button>
                    <button class="collector-btn">
                        <svg width="12" height="12" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
                        </svg>
                        Logs
                    </button>
                </div>
            </div>
        </div>
    </div>

    <script>
        // Simple demo interactions
        document.addEventListener('DOMContentLoaded', function() {
            // Handle collector button clicks
            document.querySelectorAll('.collector-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    const card = this.closest('.collector-card');
                    const action = this.textContent.trim();
                    
                    if (action === 'Start' || action === 'Retry') {
                        card.className = 'collector-card running';
                        this.textContent = 'Stop';
                        this.className = 'collector-btn stop';
                    } else if (action === 'Stop') {
                        card.className = 'collector-card stopped';
                        this.textContent = 'Start';
                        this.className = 'collector-btn start';
                    }
                });
            });

            // Handle global controls
            document.querySelector('.control-btn.primary').addEventListener('click', function() {
                document.querySelectorAll('.collector-card.stopped').forEach(card => {
                    card.className = 'collector-card running';
                });
            });

            document.querySelector('.control-btn.danger').addEventListener('click', function() {
                document.querySelectorAll('.collector-card.running').forEach(card => {
                    card.className = 'collector-card stopped';
                });
            });
        });
    </script>
</body>
</html>